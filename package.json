{
  "name": "thoughtcache",
  "version": "1.21.1",
  "description": "ThoughtCache: Cache and fetch thoughts instantly without creating an account.",
  "main": "main.js",
  "scripts": {
    "pack:osx": "electron-packager . --out=dist/macos --platform=darwin --arch=x64 --icon=source/public/favicon/favicon-128x128.icns",
    "pack:win32": "electron-packager . --out=dist/win --platform=win32 --arch=ia32",
    "pack:win64": "electron-packager . --out=dist/win --platform=win32 --arch=x64 --icon=source/public/favicon/favicon-128x128.ico",
    "build": "npm run pack:osx && npm run pack:win32 && npm run pack:win64"
  },
  "author": "Bianca Hanly, Zak Hori, Le Jeffrey Liang, Benjamin Plate",
  "license": "UNLICENSED",
  "devDependencies": {
    "electron": "^7.2.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-winstaller": "^4.0.0"
  }
}
